{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ak-rcm-project-adf"
		},
		"ak_adls_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ak_adls_ls'"
		},
		"ak_adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://akrcmprojectsa.dfs.core.windows.net/"
		},
		"ak_keyvault_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ak-rcm-project-kv.vault.azure.net/"
		},
		"ak_rcm_sqlserver_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azrcmdb.database.windows.net"
		},
		"ak_rcm_sqlserver_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().dbname}"
		},
		"ak_rcm_sqlserver_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "rcmadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ak-rcm-project-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ak_adls_ls_accountKey": {
						"value": "[parameters('ak_adls_ls_accountKey')]"
					},
					"ak_keyvault_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('ak_keyvault_ls_properties_typeProperties_baseUrl')]"
					},
					"ak_adls_ls_properties_typeProperties_url": {
						"value": "[parameters('ak_adls_ls_properties_typeProperties_url')]"
					},
					"ak_rcm_sqlserver_ls_properties_typeProperties_server": {
						"value": "[parameters('ak_rcm_sqlserver_ls_properties_typeProperties_server')]"
					},
					"ak_rcm_sqlserver_ls_properties_typeProperties_database": {
						"value": "[parameters('ak_rcm_sqlserver_ls_properties_typeProperties_database')]"
					},
					"ak_rcm_sqlserver_ls_properties_typeProperties_userName": {
						"value": "[parameters('ak_rcm_sqlserver_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}